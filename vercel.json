{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/product-price-compare",
      "dest": "/api/product-price-compare.js"
    },
    {
      "src": "/api/price-trend",
      "dest": "/api/price-trend.js"
    },
    {
      "src": "/api/region-price",
      "dest": "/api/region-price.js"
    },
    {
      "src": "/api/volume-info",
      "dest": "/api/volume-info.js"
    },
    {
      "src": "/api/tomato/price-compare",
      "dest": "/api/tomato-price-compare.js"
    },
    {
      "src": "/api/general-search",
      "dest": "/api/general-search.js"
    },
    {
      "src": "/api/origin-info",
      "dest": "/api/origin-info.js"
    },
    {
      "src": "/api/auction-info",
      "dest": "/api/auction-info.js"
    },
    {
      "src": "^/$",
      "dest": "/public/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/public/index.html"
    },
    {
      "src": "/old",
      "dest": "/public/tomato-dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
